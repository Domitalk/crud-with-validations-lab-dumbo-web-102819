<h1>EDIT SONG FORM</h1>
<ul>
<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<li>
<%= error %>
</li>
<% end %>
</ul>
<% end %>

<%= form_for @song do |f| %>
    <%= f.label "Title:" %>
    <%= f.text_field :title %>  <br>
    <%= f.label "Released:" %>
    <%= f.text_field :released %> <br>
    <%= f.label "Release Year:" %>
    <%= f.text_field :release_year %> <br>
    <%= f.label "Artist Name:" %>
    <%= f.text_field :artist_name %> <br>
    <%= f.label "Genre:" %>
    <%= f.text_field :genre %> <br>
    <%= f.submit %>
<% end %>






